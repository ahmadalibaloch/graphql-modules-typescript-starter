{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "object-literal-sort-keys": false
  },
  "rules": {
    "trailing-comma": [
      true
    ],
    "no-console": [
      false,
      "log",
      "error"
    ],
    "no-debugger": [
      true
    ],
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "linebreak-style": [
      true,
      "LF"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/types.ts"
    ]
  }
}